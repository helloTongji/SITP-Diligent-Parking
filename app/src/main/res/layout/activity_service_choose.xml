<?xml version="1.0" encoding="utf-8"?>
    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        android:paddingTop="@dimen/activity_vertical_margin"
        android:background="@drawable/yuyue"
    tools:context="com.example.admin.sitp_parkinglot.ServiceChoose"
    android:weightSum="1">

<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:text="充电服务:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="46dp"
        android:id="@+id/textView47"
        android:textSize="14sp"
        android:layout_below="@+id/textView46"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/textView46"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:checkedButton="@+id/radioButton2"
        android:id="@+id/PowerGroup">

        <RadioButton
            android:text="是"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton2"
            android:layout_weight="1"
            android:checked="true" />

        <RadioButton
            android:text="否"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton"
            android:layout_weight="1" />
    </RadioGroup>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView60" />

    <TextView
        android:text="洗车服务:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:id="@+id/textView46"
        android:textSize="14sp"
        android:layout_below="@+id/textView2"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_below="@+id/textView47"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:checkedButton="@+id/radioButton3"
        android:id="@+id/WashGroup">

        <RadioButton
            android:text="是"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton3"
            android:layout_weight="1"
            android:checked="true" />

        <RadioButton
            android:text="否"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButto4"
            android:layout_weight="1" />
    </RadioGroup>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView61" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView58" />

    <TextView
        android:text="行车检查:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView48"
        android:textSize="14sp"
        android:layout_above="@+id/radioGroup9"
        android:layout_toRightOf="@+id/textView2"
        android:layout_toEndOf="@+id/textView2" />

    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:layout_centerVertical="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:checkedButton="@+id/radioButton5"
        android:id="@+id/CheckGroup">

        <RadioButton
            android:text="是"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton5"
            android:layout_weight="1"
            android:checked="true" />

        <RadioButton
            android:text="否"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton6"
            android:layout_weight="1" />
    </RadioGroup>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView62" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView59" />

    <TextView
        android:text="车辆保养"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/radioGroup9"
        android:layout_alignRight="@+id/textView48"
        android:layout_alignEnd="@+id/textView48"
        android:id="@+id/textView51" />

    <RadioGroup
        android:layout_width="209dp"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:id="@+id/ProtectGroup">

        <RadioButton
            android:text="是"
            android:layout_width="120dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton8"
            android:layout_weight="1"
            android:checked="true" />

        <RadioButton
            android:text="否"
            android:layout_width="90dp"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton7"
            android:layout_weight="1" />
    </RadioGroup>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView63" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView53" />

    <TextView
        android:text="车位类型:"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView64" />

    <RadioGroup
        android:layout_width="349dp"
        android:layout_height="31dp"
        android:orientation="horizontal"
        android:id="@+id/CarSize">

        <RadioButton
            android:text="大型车位"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton10"
            android:layout_weight="1" />

        <RadioButton
            android:text="中型车位"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton9"
            android:layout_weight="1" />

        <RadioButton
            android:text="小型车位"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/radioButton5"
            android:layout_weight="1"
            android:checked="true" />
    </RadioGroup>

    <TextView
        android:text="车牌号:"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView3"
        android:layout_weight="1" />

    <EditText
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:inputType="textPersonName"
        android:background="@drawable/edit_bg"
        android:ems="10"
        android:id="@+id/editText11" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textView57" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="344dp"
        android:layout_height="38dp">

        <Button
            android:text="预约"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/buttonEnsure"
            android:background="@drawable/btn_bg"
            android:textColor="@android:color/white"
            android:layout_weight="1" />

    </LinearLayout>

    <TextView
        android:text=" "
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="53dp"
        android:id="@+id/textView2"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</LinearLayout>
    </ScrollView>